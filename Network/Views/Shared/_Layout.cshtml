@model ViewModel
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@Model.Title</title>
    <link type="image/x-icon" rel="shortcut icon" href="/favicon.ico"/>
    <meta content="@Model.Description" name="description"/>
    <meta content="@Model.Keywords" name="keywords" />
    <link href="@Url.Content("~/Content/Network_Site.css")" rel="stylesheet" type="text/css" />
    @RenderSection("Css", false)
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.js")" type="text/javascript"></script>
</head>
    <!--[if lt IE 9]> <script src="@Url.Content("~/Scripts/css3-mediaqueries.js")" type="text/javascript"></script> <![endif]-->
    <!--[if IE 6]>
    <script src="@Url.Content("~/Scripts/png.js")" type="text/javascript" ></script>
     <script type="text/javascript">
     DD_belatedPNG.fix('.png');
    </script> 
    <script language='javascript' type="text/javascript">   
    function ResumeError() {  
         return true;  
    }  
    window.onerror = ResumeError;   
    </script> 
    <![endif]-->
<body>
    @Html.Partial("_Header", Model)
    @RenderBody()
    @Html.Partial("_Footer")
    @RenderSection("Js", false)
    <script type="text/javascript">
        var isie6 = $.browser.msie && ($.browser.version == "6.0");
        $(document).ready(function () {
            $(".Nav li[id!=navBottom]").mousemove(function () {
                if ($(this).children().length > 1)
                    $("#navBottom").css("top", "40px");
                if (isie6)
                    $(this).find(".subMenu").css("height", "200px");
            }).mouseout(function () {
                if ($(this).children().length > 1)
                    $("#navBottom").css("top", "35px");
                if (isie6)
                    $(this).find(".subMenu").css("height", "0px");
            });
        });
    </script>
    <script>
            var _hmt = _hmt || [];
            (function () {
                var hm = document.createElement("script");
                hm.src = "//hm.baidu.com/hm.js?adccf0b7f9e7d88abc3707b91449312f";
                var s = document.getElementsByTagName("script")[0];
                s.parentNode.insertBefore(hm, s);
            })();
    </script>

</body>
</html>
